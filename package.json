{
  "name": "svelte-droplet",
  "version": "0.2.0",
  "description": "File dropzone for Svelte",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit package && vite build",
    "build:package": "svelte-kit package",
    "build:site": "vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir=. --check src *.js && eslint src *.js && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --plugin-search-dir=. --write src *.js && eslint --fix src *.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.48",
    "@sveltejs/kit": "1.0.0-next.551",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "sass": "^1.56.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "license": "MIT",
  "homepage": "https://svelte-droplet.kasper.space",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/probablykasper/svelte-droplet.git"
  },
  "bugs": {
    "url": "https://github.com/probablykasper/svelte-droplet/issues"
  },
  "author": {
    "name": "Kasper Henningsen",
    "url": "https://kasper.space"
  },
  "keywords": [
    "svelte",
    "file",
    "filedrop",
    "drag",
    "drop",
    "dropzone",
    "upload",
    "input",
    "component"
  ],
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  }
}
